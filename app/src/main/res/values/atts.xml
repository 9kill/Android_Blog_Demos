<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CubeTextView">
        <attr name="textBackgroud" format="color"></attr>
        <attr name="spacingBackgroup" format="color"></attr>
        <attr name="textSize" format="dimension"></attr>
        <attr name="textColor" format="color"></attr>
    </declare-styleable>
    <declare-styleable name="AddAndSubtractEditTextView">
        <attr name="inputTypeAccuracy" format="string">
            <enum name="number" value="0"/><!--最小增长大小 1-->
            <enum name="numberDecimal" value="1"/><!--最小增长大小 0.1-->
        </attr>
        <attr name="unitName" format="string"/>
        <attr name="inputTextValue" format="string"/>
        <attr name="inputTextColor" format="color"/>
        <attr name="inputTextSize" format="dimension"/>
    </declare-styleable>
</resources>